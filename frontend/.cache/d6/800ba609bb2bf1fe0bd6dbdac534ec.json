"line":47,"column":6}},{"generated":{"line":102,"column":239},"source":"components/Generations.js","original":{"line":46,"column":4}},{"generated":{"line":103,"column":0},"source":"components/Generations.js","original":{"line":52,"column":3}},{"name":"Component","generated":{"line":107,"column":2},"source":"components/Generations.js","original":{"line":6,"column":25}},{"generated":{"line":109,"column":0},"source":"components/Generations.js","original":{"line":53,"column":1}},{"name":"Generation","generated":{"line":110,"column":15},"source":"components/Generations.js","original":{"line":55,"column":15}}],"sources":{"components/Generations.js":"import React, { Component } from 'react';\n\nconst DEFAULT_GENERATION = { generationId: '', expiration: ''};\nconst MINIMUM_DELAY = 3000;\n\nclass Generation extends Component {\n  state = { \n    generation: DEFAULT_GENERATION \n  };\n\n  timer = null;\n\n  componentDidMount() {\n    this.fetchNextGeneration();\n  }\n\n  componentWillUnmount() {\n    clearTimeout(this.timer);\n  }\n\n  fetchGeneration = () => {\n    fetch('http://localhost:3000/generation')\n    .then(response => \n    response.json())\n    .then(json => { console.log('json',    json)\n    this.setState({ generation: json.generation })\n  })\n    .catch(error => console.error('error', error));\n  };\n\n  fetchNextGeneration = () => {\n    this.fetchGeneration();\n\n    let delay = new Date(this.state.generation.expiration).getTime() - new Date().getTime(); \n\n    if (delay < MINIMUM_DELAY) {\n      delay = MINIMUM_DELAY;\n    };\n\n    this.timer = setTimeout(() => this.fetchNextGeneration(), delay); \n  };\n\n  render() {\n    const { generation } = this.state;\n\n    return (\n      <div>\n        <h3>Generation {generation.generationId}. Expires on:</h3>\n        <h4>{new Date(generation.expiration).toString()}</h4>\n      </div>\n    )\n  }\n};\n\nexport default Generation;"},"lineCount":null}},"error":null,"hash":"6ac3dbfed21d9b1fc626e03064a903e7","cach